{
  "name": "resume",
  "version": "0.0.1",
  "author": "刘功明",
  "description": "从0到1实现一款轻巧适用的简历平台桌面应用。",
  "main": "./dist/electron.js",
  "scripts": {
    "start:main": "webpack --config ./webpack/main.dev.js && electron ./dist/main.js",
    "start:render": "webpack serve --config ./webpack/render.dev.js",
    "build:main": "webpack --config ./webpack/main.prod.js",
    "build:render": "webpack --config ./webpack/render.prod.js",
    "install:electron": "ELECTRON_MIRROR=https://cdn.npm.taobao.org/dist/electron/ npm install electron -D",
    "lint": "eslint --ext .js,.jsx ./**/* --fix",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "electron": "^16.0.5",
    "electron-builder": "^22.14.5",
    "eslint": "^8.5.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "html-webpack-plugin": "^5.5.0",
    "prettier": "^2.5.1",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.1",
    "webpack-merge": "^5.8.0"
  },
  "build": {
    "appId": "visResumeMook.ElectronReact",
    "productName": "VisResumeMook",
    "copyright": "Copyright © 2019 ${author}",
    "files": [
      "dist/**/*",
      "package.json",
      "node_modules/"
    ],
    "win": {
      "target": [
        "nsis",
        "msi"
      ]
    }
  }
}
